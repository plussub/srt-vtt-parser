{
  "name": "@plussub/srt-vtt-parser",
  "version": "1.1.0",
  "description": "A dependency free SRT / Vtt subtitle parser, written in Typescript.\n",
  "keywords": [
    "vtt",
    "srt",
    "webvtt",
    "subtitle",
    "captions",
    "parser"
  ],
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "tsc",
    "ts-check": "tsc --noEmit --skipLibCheck",
    "test": "jest",
    "shortcut": "npm run clean && npm run build && npm version patch && git push && git push --tags && npm pack && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plussub/srt-vtt-parser.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plussub/srt-vtt-parser/issues"
  },
  "homepage": "https://github.com/plussub/srt-vtt-parser#readme",
  "publishConfig": {
    "plussub:registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@types/jest": "^26.0.7",
    "jest": "^27.0.6",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.4",
    "tslib": "^2.0.0",
    "typescript": "^4.3.5"
  },
  "files": [
    "dist/**/*"
  ],
  "prettier": {
    "trailingComma": "none",
    "printWidth": 200,
    "singleQuote": true
  }
}
